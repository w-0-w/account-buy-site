{
  "name": "account-buy-site",
  "version": "1.0.0",
  "description": "Site.",
  "scripts": {
    "start": "ice start --speedup",
    "mock": "serve -l 3344 public -C",
    "s": "concurrently \"npm run mock\" \"npm start\"",
    "build": "ice build",
    "server": "serve -l 3456 build --no-request-logging",
    "server:pure": "serve -l 3456 build",
    "node:server": "node serve.js",
    "deploy": "git pull && npm i && npm run build && npm run node:server",
    "eslint": "eslint ./src --cache --ext .js,.jsx,.ts,.tsx",
    "eslint:fix": "npm run eslint -- --fix",
    "stylelint": "stylelint \"src/**/*.{css,scss,less}\" --cache",
    "stylelint:fix": "npm run stylelint -- --fix"
  },
  "dependencies": {
    "@alifd/next": "^1.26.8",
    "@alifd/theme-design-pro": "^0.8.0",
    "@ice/runtime": "^1.4.0",
    "ahooks": "^3.8.4",
    "moment": "^2.28.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.8.9",
    "serve": "^14.2.4"
  },
  "devDependencies": {
    "@applint/spec": "^1.2.3",
    "@ice/app": "^3.4.0",
    "@ice/plugin-i18n": "^5.0.1",
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "concurrently": "^9.1.2",
    "eslint": "^8.35.0",
    "stylelint": "^15.2.0",
    "typescript": "5.8.3"
  }
}
